{
  "name": "@optimizely/optimizely-sdk",
  "version": "3.6.0-alpha.1",
  "description": "JavaScript SDK for Optimizely X Full Stack",
  "main": "dist/optimizely.node.js",
  "browser": "dist/optimizely.browser.js",
  "react-native": "lib/optimizely.react_native.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "test": "mocha ./lib/*.tests.js ./lib/**/*.tests.js ./lib/**/**/*tests.js --recursive --exit --require esm --require lib/tests/exit_on_unhandled_rejection.js",
    "test-xbrowser": "karma start karma.bs.conf.js --single-run",
    "test-umdbrowser": "npm run build-browser-umd && karma start karma.umd.conf.js --single-run",
    "build": "node ./scripts/build.js && babel lib/index.browser.js lib/index.browser.*.js --out-dir lib --presets=@babel/env",
    "build-browser-umd": "npm run build && rm -rf dist && webpack",
    "test-ci": "npm run build && npm run test-xbrowser && npm run test-umdbrowser",
    "lint": "eslint 'lib/**/*.js'",
    "cover": "istanbul cover _mocha ./lib/*.tests.js ./lib/**/*.tests.js ./lib/**/**/*tests.js",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "prepublishOnly": "npm run build-browser-umd && npm test && npm run test-xbrowser && npm run test-umdbrowser"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/optimizely/javascript-sdk.git"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=4.0.0"
  },
  "keywords": [
    "optimizely"
  ],
  "bugs": {
    "url": "https://github.com/optimizely/javascript-sdk/issues"
  },
  "homepage": "https://github.com/optimizely/javascript-sdk/tree/master/packages/optimizely-sdk",
  "dependencies": {
    "5to6-codemod": "^1.8.0",
    "@optimizely/js-sdk-datafile-manager": "^0.4.0",
    "@optimizely/js-sdk-event-processor": "^0.4.0",
    "@optimizely/js-sdk-logging": "^0.1.0",
    "@optimizely/js-sdk-utils": "^0.2.0",
    "json-schema": "^0.2.3",
    "murmurhash": "0.0.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "bluebird": "^3.4.6",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "eslint": "^6.7.2",
    "esm": "^3.2.25",
    "istanbul": "^0.4.5",
    "json-loader": "^0.5.4",
    "karma": "^3.1.4",
    "karma-browserstack-launcher": "^1.5.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-mocha": "^1.3.0",
    "karma-sinon": "^1.0.5",
    "karma-webpack": "^3.0.5",
    "lodash": "^4.17.11",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nock": "^7.7.2",
    "promise-polyfill": "8.1.0",
    "rollup": "^2.2.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-terser": "^5.3.0",
    "sinon": "^2.3.1",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/",
    "lib/",
    "LICENSE",
    "CHANGELOG",
    "README.md",
    "package.json"
  ]
}
